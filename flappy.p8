pico-8 cartridge // http://www.pico-8.com
version 41
__lua__

JUMP_FORCE = 7
GRAVITY = 1
PIPE_SPEED = 2
pipe_gap = 40 + rnd(32)
pipe_sprite_height = 16 * 8
pipe_coordinates = {}
score = 0
collision_tolerance = 3
collides = false
player_sprite = 17
scene = 'intro'

function _init()
    x = 12
    y = 64
    vy = 0
    add(pipe_coordinates, { x = 64, y = 64, name = 'bottom' })
    add(pipe_coordinates, { x = 64, y = -64, name = 'top' })
    init_pipes()
end

function _update()
    if scene == 'game' then update_game() end
    if scene == 'intro' then update_intro() end
    if scene == 'fail' then update_fail() end
end

function update_intro()
    if btnp(5) or btnp(6) then scene = 'game' end
end

function update_fail()
    if btnp(5) or btnp(6) then
        score = 0
        init_pipes()
        scene = 'game'
    end
end

function update_game()
    if btnp(5) then vy = JUMP_FORCE end
    if vy > 0 then vy = vy - 1 end
    y = y + GRAVITY - vy
    if y > 120 then y = 120 end
    if y < 0 then y = 0 end
    for pipe in all(pipe_coordinates) do
        pipe.x -= PIPE_SPEED
    end
    if pipe_coordinates[1].x < 0 then init_pipes() end
    update_score()
    test_collisions()
    if collides then scene = 'fail' end
    if btn(5) then
        player_sprite = 16
    else
        player_sprite = 17
    end
end

function _draw()
    if scene == 'game' then draw_game() end
    if scene == 'intro' then draw_intro() end
    if scene == 'fail' then draw_fail() end
end

function draw_intro()
    cls(13)
    sspr(8 * 8, 0, 32, 32, 32, 0, 64, 64)
    print('press any button to begin', 14, 64)
end

function draw_fail()
    cls(13)
    print('score: ' .. score, 48, 58, 2)
    print('game over', 48, 64, 1)
    print('press any button to restart', 12, 70, 1)
end

function draw_game()
    cls(5)
    map()
    for pipe in all(pipe_coordinates) do
        draw_pipes(pipe.x, pipe.y)
    end
    -- draw player
    draw_player()
    print("score: " .. score)
end


function draw_player() 
    if btn(5) 
        then spr(8, x, y, 4, 4)
        else spr(12, x, y, 4, 4) 
    end 
end

function init_pipes()
    for pipe in all(pipe_coordinates) do
        pipe.x = 128
    end
    pipe_gap = 40 + rnd(32)
    pipe_height_1 = rnd(pipe_sprite_height / 2) + 4
    -- set bottom coordinates
    pipe_coordinates[1].y = 0 - (pipe_sprite_height - pipe_height_1)
    --pipe_height - pipe_height_1
    pipe_coordinates[2].y = pipe_coordinates[1].y + pipe_sprite_height + pipe_gap
end

function draw_pipes(cx, cy)
    spr(2, cx, cy, 2, 16)
end

-->8
--score--

function max(a, b)
    if a > b then
        return a
    end
    return b
end

function update_score()
    if abs(pipe_coordinates[1].x - x) < PIPE_SPEED then
        score += 1
    end
end

function test_collisions()
    if x > pipe_coordinates[1].x and x < pipe_coordinates[1].x + 16 then
        -- top
        if y > pipe_coordinates[1].y and y < pipe_coordinates[1].y + pipe_sprite_height then
            maxY = max(
                abs(pipe_coordinates[1].y - y),
                abs(y - pipe_coordinates[1].y)
            )
            if maxY > collision_tolerance then
                collides = true
                return
            end
        end
        -- bottom
        if y > pipe_coordinates[2].y then
            if abs(y - pipe_coordinates[2].y) > collision_tolerance then
                collides = true
                return
            end
        end
    end
    collides = false
    return
end

